#include <stdio.h>

int main() {
  int a[10],n,i,sum=0;
  printf(" enter array size:");
  scanf("%d",&n);
  printf("\nenter array elements:");
  for(i=0;i<n;i++)
  {
      scanf("%d",&a[i]);
  }
  for(i=0;i<n;i++)
  {
      sum=sum+a[i];
  }
  printf("\nthe sum of elements of array is :%d",sum);
    return 0;
}
