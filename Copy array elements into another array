#include <stdio.h>

int main() {
  int a[10],n,i,b[10];
  printf(" enter array size:");
  scanf("%d",&n);
  printf("\nenter array elements:");
  for(i=0;i<n;i++)
  {
      scanf("%d",&a[i]);
  }
  for(i=0;i<n;i++)
   {
      b[i]=a[i];
  }
  for(i=0;i<n;i++)
  {
  printf("%d\t",b[i]);
  }
    return 0;
}
