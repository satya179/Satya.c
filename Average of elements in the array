#include <stdio.h>

int main() {
  int a[10],n,i,sum=0;
  float avg=0;
  printf(" enter array size:");
  scanf("%d",&n);
  printf("\nenter array elements:");
  for(i=0;i<n;i++)
  {
      scanf("%d",&a[i]);
  }
  for(i=0;i<n;i++)
  {
      sum=sum+a[i];
  }
  avg=sum/n;
  printf("\nthe average of elements in array is :%f",avg);
    return 0;
}
